macro p set browser "openfeed-preview" ; open-in-browser ; set browser "openfeed-default"
macro b set browser "openfeed-browser" ; open-in-browser ; set browser "openfeed-default"
macro v set browser "openfeed-video" ; open-in-browser ; set browser "openfeed-default"
macro q set browser "openfeed-qr" ; open-in-browser ; set browser "openfeed-default"
macro d set browser "openfeed-download" ; open-in-browser ; set browser "openfeed-default"

reload-threads 2
