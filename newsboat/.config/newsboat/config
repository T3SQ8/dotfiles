bind-key j down
bind-key k up
bind-key l open
bind-key h quit
bind-key G end
bind-key g home
unbind-key ,
bind-key SPACE macro-prefix
bind-key I open-all-unread-in-browser
bind-key H toggle-show-read-feeds
bind-key c rev-sort

browser "openfeed-default"
