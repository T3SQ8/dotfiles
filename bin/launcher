#!/bin/sh

gui_programs="keepassxc pcmanfm chromium firefox"
terminal_programs="nvim ranger newsboat"

Choise=$(echo "$gui_programs" "$terminal_programs" | tr " " "\n" | sort | dmenu)

[ -z "$Choise" ] && exit

echo "$gui_programs" | grep -wq "$Choise" &&
	sh -c "$Choise" &

echo "$terminal_programs" | grep -wq "$Choise" &&
	$TERMINAL -e "$Choise" &
